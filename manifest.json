{
    "manifest_version": 2,
    "name": "Color Vision Corrector",
    "version": "1.0",
    "description": "Corrects color vision deficiencies in images.",
    "permissions": ["activeTab", "storage", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  
